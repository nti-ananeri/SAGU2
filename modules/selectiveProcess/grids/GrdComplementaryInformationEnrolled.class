/**
 * Grid used by form to display search results
 **/
class GrdComplementaryInformationEnrolled extends MGrid
{
    /**
     * Class constructor
     **/
    function __construct($filters)
    {
        $MIOLO  = MIOLO::getInstance();
        $module = MIOLO::_request('module');
        $action = MIOLO::getCurrentAction();
        $selectiveProcessId = MIOLO::_request('selectiveProcessId');
        $reportOption = MIOLO::_request('reportOption');
        /*
         * MGridColumn constructor parameters:
         *    $title = ''
         *    $align = 'left'
         *    $nowrap = false
         *    $width = 0
         *    $visible = true
         *    $options = null
         *    $order = false
         *    $filter = false
         */

        $columns = array(
                        new MDataGridColumn('inscriptionId', _M('Inscription id', $module),  'right', true, '', true),
                        new MDataGridColumn('personName', _M('Person name', $module),  'left', true, '', true)
                        );

        foreach ( $columns as $k => $v )
        {
            $v = 'ata';
        }

        unset($opts);
        

        $url = $MIOLO->getCurrentURL();
        parent::__construct( null, $columns, $url, null, 0, 'gridComplementaryInformationEnrolled' );
        $this->setTitle( _M('Enrolled by city', $module));
        $this->setIsScrollable();
        $this->eventHandler();
    }

}

