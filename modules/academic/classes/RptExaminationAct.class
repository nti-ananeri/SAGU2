	public function examinationActPupilLine($count, $personId, $personName, $courseId, $dateCancellation = null, $enrollId=null, $degreeId=null, $degreeQualityId=null)
	{
		$module = 'academic';
		$MIOLO  = MIOLO::getInstance();

		$this->setLineWidth(0.1);
		$this->setFont(DEFAULT_REPORT_FONT, '', 8);
		$this->cell(5, 6, $count, 1, 0, 'C');
		$this->cell(15, 6, $personId, 1, 0, 'C');
		$this->cell(80 ,6, $personName, 1, 0);
		$this->cell(18, 6, $courseId, 1, 0, 'C');

		$busFrequenceEnroll = $MIOLO->getBusiness('academic', 'BusFrequenceEnroll');
		$totalfrequencies   = $busFrequenceEnroll->getTotalFrquenceEnroll($enrollId);
		$totalabsencies          = $busFrequenceEnroll->getTotalAbsenceEnroll($enrollId);

		//pega a nota parcial
		if ( $degreeId && $enrollId )
		{
			$busDegreeEnroll = $MIOLO->getBusiness('academic', 'BusDegreeEnroll');
			$notes           = $busDegreeEnroll->getDegreeEnroll($degreeId, $enrollId);
			if($notes->note !='--')
			{
				$this->cell(20, 6, number_format($notes->note,2,',','.'), 1, 0, 'C');
			}
			else
			{
				$this->cell(20, 6, $notes->note, 1, 0, 'C');
			}
			$media = $notes->note;
		}
		//pega a média final
		elseif ( $enrollId )
		{
			$busEnroll = $MIOLO->getBusiness('academic', 'BusEnroll');
			$notes     = $busEnroll->getEnroll($enrollId);
			if($notes->note != '--')
			{
				$this->cell(20, 6, number_format($notes->note,2,',','.'), 1, 0, 'C');
			}
			else
			{
				$this->cell(20, 6, $notes->note, 1, 0, 'C');
			}
		}
		//deixa em branco
		else
		{
			$this->cell(20, 6, '', 1, 0, 'C');
		}

		//pega a nota do qualitativo
		if ($degreeQualityId)
		{
			$busDegreeEnroll = $MIOLO->getBusiness('academic', 'BusDegreeEnroll');
			$notes           = $busDegreeEnroll->getDegreeEnroll($degreeQualityId, $enrollId);
			if($notes->note != '--')
			{
				$this->cell(20, 6, number_format($notes->note,2,',','.'), 1, 0, 'C');
				$media = $media + $notes->note;
				$this->cell(10, 6, number_format($media,2,',','.'), 1, 0, 'C');
			}
			else
			{
				$this->cell(20, 6, $notes->note, 1, 0, 'C');
				$media = $notes->note;
				$this->cell(10, 6, $media, 1, 0, 'C');
			}
		}

		//senao, pega a frequencia parcial
		elseif ( $degreeId && $enrollId )
		{
			//$busFrequenceEnroll = $MIOLO->getBusiness('academic', 'BusFrequenceEnroll');
			$frequencies        = $busFrequenceEnroll->getTotalFrquenceEnroll($enrollId, $degreeId);
			//$absencies          = $busFrequenceEnroll->getTotalAbsenceEnroll($enrollId, $degreeId);
			$this->cell(20, 6, $frequencies, 1, 0, 'C');
		}
		//pega a frequencia total
		elseif ( $enrollId )
		{
			//$busFrequenceEnroll = $MIOLO->getBusiness('academic', 'BusFrequenceEnroll');
			$frequencies        = $busFrequenceEnroll->getTotalFrquenceEnroll($enrollId);
			//$absencies          = $busFrequenceEnroll->getTotalAbsenceEnroll($enrollId, $degreeId);
			$this->cell(20, 6, $frequencies, 1, 0, 'C');
		}
		//deixa em branco
		else
		{
			$this->cell(20, 6, '', 1, 0, 'C');
		}

		for ($x = 0; $x < 20; $x++)
		{

			if ($x == 10)
			{
				$cx = $this->getX();
				$cy = $this->getY();
			}
		}

		if ( $degreeQualityId )
		{
			$this->cell(110, 6, '', 1, 0);
		}
		else
		{
			$this->cell(120, 6, '', 1, 0);
		}

		if ($dateCancellation != null)
		{
			$this->setXY($cx, $cy);
			$this->setFont(DEFAULT_REPORT_FONT, '', 7);
			$this->cell(100, 6, _M('Locked in', $module) . ' ' . $dateCancellation, 0, 0);
		}
		else if ($totalabsencies > ($this->CH - $this->MinHours))
		{

			$this->setXY($cx, $cy);
			$this->setFont(DEFAULT_REPORT_FONT, '', 5);
			$this->cell(100, 6, _M('Atencao: faltas em excesso: '.$totalabsencies, $module), 0, 0);
			$this->setFont(DEFAULT_REPORT_FONT, '', 7);
		}

		$this->ln();
	}
}


/**
 * PDF extension to generate report
 */
class RptExaminationAct
{
	var $listSpace;

	public function RptExaminationAct($data = null)
	{
		$MIOLO       = MIOLO::getInstance();
		$module      = 'academic';

		$business    = $MIOLO->getBusiness('academic', 'BusAttendance');
			
		// Generating PDF file
		$pdf = new PDF('P','mm','A4');
		$pdf->aliasNbPages();
		//$pdf->personName = $rdata->name;
		// First page
		$pdf->setTopMargin(1);

		/* ISSO AQUI É PARA AS MEDIAS (GRAUS) DO SEMESTRE */
		$reg = 0;
		if (count($data->groupId) > 0)
		{
			foreach ($data->groupId as $group)
			{
				$scheduleId          = $data->scheduleId;
				$scheduleProfessorId = $data->scheduleProfessorId;

				if ( count($group) > 1 )
				{
					list (  $groupId,
					$name,
					$weekDayId,
					$unitId,
					$turnId,
					$scheduleId
					) = $group;
				}
				else
				{
					$groupId = $group[0];
				}

				$reportData  = $business->getGroupData($groupId, $data->scheduleId, $data->scheduleProfessorId);
				$reportData->emissionDate = $data->emissionDate;
				$reportData->pupils       = $business->listGroupEnrolledPupils($groupId);

				$filter->learningPeriodId = $reportData->learningPeriodId;
				$busDegrees = $MIOLO->getBusiness('academic', 'BusDegree');
				$degrees    = $busDegrees->searchDegreeQuality($filter);

				//aqui tem que ter um foreach dos graus
				if ( count($degrees) )
				{
					foreach ( $degrees as $degree )
					{
						$degreeId          = $degree[0];
						$degreeDescription = $degree[6];
						$degreeNumber      = $degree[9];
						$degreeQualityId   = $degree[17];

						if ($degreeNumber % 2 != 0) // Pega só os graus impares: os graus pares são qualitativos... (ana neri)
						{
							$pdf->addPage('L', 'mm', 'A4');
							$pdf->examinationActHeader($reportData, $reg, $degreeId);
							$pdf->examinationActColumns($degreeId, $degreeDescription, $degreeQualityId);
							$this->listSpace = 100;

							//Pupils listing
							if ( count($reportData->pupils) > 0)
							{
								$count = 1;
								$reg2  = 0;

								foreach ($reportData->pupils as $pupil)
								{
									list ($enrollId,
									$personId,
									$personName,
									$courseId,
									$statusId,
									$dateCancellation) = $pupil;

									if ($this->listSpace < 5)
									{
										$pdf->examinationActFooter();
										$pdf->addPage('L', 'mm', 'A4');
										$pdf->examinationActHeader($reportData, $reg2, $degreeId);
										$pdf->examinationActColumns($degreeId, $degreeDescription,$degreeQualityId);
										$this->listSpace = 100;
									}

									if ($statusId == ENROLL_STATUS_CANCELLED ||
									$statusId == ENROLL_STATUS_DESISTING )
									{
										$pdf->examinationActPupilLine('*', $personId, $personName, $courseId, $dateCancellation, $enrollId, $degreeId, null);
									}
									else
									{
										$pdf->examinationActPupilLine($count, $personId, $personName, $courseId, null, $enrollId, $degreeId, $degreeQualityId);
										$count++;
									}
									$this->listSpace = $this->listSpace - 6;
									$reg2++;
								}
							}
							$pdf->examinationActFooter();
						}
					}
					$reg++;
				}
			}
		}

		/* ISSO AQUI É PARA A MEDIA FINAL */
		$reg = 0;
		if (count($data->groupId) > 0)
		{
			foreach ($data->groupId as $group)
			{
				$scheduleId          = $data->scheduleId;
				$scheduleProfessorId = $data->scheduleProfessorId;

				if ( count($group) > 1 )
				{
					list (  $groupId,
					$name,
					$weekDayId,
					$unitId,
					$turnId,
					$scheduleId
					) = $group;
				}
				else
				{
					$groupId = $group[0];
				}

				$reportData  = $business->getGroupData($groupId, $data->scheduleId, $data->scheduleProfessorId);
				$reportData->emissionDate = $data->emissionDate;
				$reportData->pupils       = $business->listGroupEnrolledPupils($groupId);

				$pdf->addPage('L', 'mm', 'A4');
				$pdf->examinationActHeader($reportData, $reg);
				$pdf->examinationActColumns();
				$this->listSpace = 100;

				//Pupils listing
				if ( count($reportData->pupils) > 0)
				{
					$count = 1;
					$reg2  = 0;

					foreach ($reportData->pupils as $pupil)
					{
						list ($enrollId,
						$personId,
						$personName,
						$courseId,
						$statusId,
						$dateCancellation) = $pupil;

						if ($this->listSpace < 5)
						{
							$pdf->examinationActFooter();
							$pdf->addPage('L', 'mm', 'A4');
							$pdf->examinationActHeader($reportData, $reg2);
							$pdf->examinationActColumns();
							$this->listSpace = 100;
						}

						if ($statusId == ENROLL_STATUS_CANCELLED ||
						$statusId == ENROLL_STATUS_DESISTING )
						{
							$pdf->examinationActPupilLine('*', $personId, $personName, $courseId, $dateCancellation, $enrollId,null);
						}
						else
						{
							$pdf->examinationActPupilLine($count, $personId, $personName, $courseId, null, $enrollId,null);
							$count++;
						}
						$this->listSpace = $this->listSpace - 6;
						$reg2++;
					}
				}
				$pdf->examinationActFooter();
			}
		}
		$reg++;

		$pdf->close();
		$pdf->output();
	}
}

?>
