//Complementação curricular
        if ( count($rdata[14]) > 0 )
        {
            $this->pdf->ln();
            $this->pdf->setFont(DEFAULT_REPORT_FONT, 'B', 7);
            $this->pdf->SetLineWidth('0.5mm');
            $this->pdf->cell(192, 5, _M('CURRICULAR COMPONENTS TO LESSON HOURS COMPLEMENT', $module), 'BT', 0, 'C');
            $this->pdf->ln();

            $this->pdf->SetLineWidth('0.5mm');
            $this->pdf->setFont(DEFAULT_REPORT_FONT, '', 6);
            $this->pdf->cell(8, 5, _M('Per', $module), 'BT', 0, 'L');
            $this->pdf->cell(10, 5, _M('Course', $module), 'BT', 0, 'L');
            $this->pdf->cell(140, 5, _M('Curricular component', $module), 'BT', 0, 'L');
            $this->pdf->cell(8, 5, _M('Grade', $module), 'BT', 0, 'L');
            $this->pdf->cell(7, 5, _M('H.L.', $module), 'BT', 0, 'L');
            $this->pdf->cell(8, 5, _M('Cred.', $module), 'BT', 0, 'L');
            $this->pdf->cell(11, 5, _M('Cour.', $module), 'BT', 0, 'L');
            $this->pdf->ln();

            foreach ( $rdata[14] as $inscriptionLHC )
            {
                if ( $x == 1 )
                {
                    $x = 0;
                }
                else
                {
                    $x = 1;
                }
                $y++;

                $this->pdf->cell(8, 3, $inscriptionLHC[0], 0, 0, 'L');
                $this->pdf->cell(8, 3, $inscriptionLHC[16] . '/' . $inscriptionLHC[17], 0, 0, 'L');
                $this->pdf->cell(142 , 3, substr(($inscriptionLHC[1] . ' ' . $inscriptionLHC[10]), 0, 110), 0, 0, 'L');
                $this->pdf->cell(8, 3, $inscriptionLHC[6], 0, 0, 'L');
                $this->pdf->cell(7, 3, $inscriptionLHC[14], 0, 0, 'L');
                $this->pdf->cell(8, 3, SAGU::calcNumber($inscriptionLHC[15], true), 0, 0, 'L');
                $this->pdf->cell(10, 3, $inscriptionLHC[8], 0, 0, 'L');

                $this->pdf->ln();
            }
            if ($yy != 0)
            {
                $this->pdf->ln();
            }
        }

        //Bloco para escrever os cursos que o aluno já concluiu
        $this->pdf->ln(2);

        if ( is_array($rdata[15]) && ( count($rdata[15]) > 0 ) )
        {
            $this->pdf->setFont(DEFAULT_REPORT_FONT, 'B', 7);
            foreach ( $rdata[15] as $value1 )
            {
                $this->pdf->cell(192, 0, _M('OBS', $module) . '.: ' . _M('This pupil is formed in @1 at @2.', $module, ($value1[1] . ' - ' . $value1[2]), $legalPerson->name), '', 0, '') ;

                $this->pdf->ln(3);
            }
        }

        if ( $rdata[7] != null )
        {
            $fill = 0;
            foreach ( $rdata[7] as $message )
            {

                if ( $message[1] = 'B' )
                {
                    $this->pdf->setFont(DEFAULT_REPORT_FONT, 'B', 7);
                }
                elseif ( $message[1] = 'I' )
                {
                    $this->pdf->setFont(DEFAULT_REPORT_FONT, 'I', 7);
                }
                elseif ( $message[1] = 'D' )
                {
                    $this->pdf->setFont(DEFAULT_REPORT_FONT, '', 7);
                }

                $contract = $rdata[6][0][0];

                if ( $fill == 0 )
                {
                    $this->pdf->cell(8, 3, '', 'T', 0, 'L');
                    $this->pdf->MultiCell(184, '3', str_replace("\n", '', str_replace("\$preco", number_format($message[3],2, ',', '.'), str_replace("\$contrato", "$contract", $message[0]))), 'T', 'L', 0);
                }
                elseif ( $fill == 1 )
                {
                    $this->pdf->cell(8, 3, '', 0, 0, 'L');
                    $this->pdf->MultiCell(184, '3', str_replace("\n", '', str_replace("\$preco", number_format($message[3],2, ',', '.'), str_replace("\$contrato", "$contract", $message[0]))), 0, 'L', 0);
                }

                $fill = 1;
            }
        }

        $yy = 0;
        $this->pdf->setFont(DEFAULT_REPORT_FONT, 'B', 7);
        $this->pdf->SetLineWidth('0.5mm');

        if ( $rdata[5] != null )
        {
            unset($docFinal);
            foreach ( $rdata[5] as $doc )
            {
                $docFinal .= $doc[1] . ', ';
            }

            $docFinal = substr($docFinal, 0, strlen((string)$docFinal)-2);

            $this->pdf->cell(192, 5, _M('HANGING DOCUMENTS', $module) . ': ' . $docFinal, 'T', 0, 'L');
        }
        else
        {
            $this->pdf->cell(45, 5, _M('Without pendencies at the moment', $module), 0, 0, 'L');
        }

        $this->pdf->ln();

        $x = 1;
        $this->pdf->setFont(DEFAULT_REPORT_FONT, 'B', 6);
        $this->pdf->SetLineWidth('0.2mm');
        $this->pdf->cell(192, 4, _M('RESPONSIBLE FOR THE PAYMENT OF THE MONTHLY FEES (Deep Support to Student)', $module) . ': ' . $rdata[6][0][0] . ' - ' . $rdata[6][0][1], 'BT', 0, 'L'/*, $x*/);

        $this->pdf->ln();

        $businessCity = $MIOLO->getBusiness('basic', 'BusCity');
        $city         = $businessCity->getCity($legalPerson->cityId);

        $this->pdf->setFont(DEFAULT_REPORT_FONT, '', 6);
        $this->pdf->SetLineWidth('0.5mm');
        $this->pdf->cell(192, 4, _M('Pupil', $module) . ': ' . ' ________________________________________  ' . _M('Received by', $module) . ': ' . ' ________________________________________  ' . SAGU::getCapitulatetString($city->name) . ' ___/___/______  ', 'B', 0, 'L');
        $this->pdf->ln();

        $this->pdf->SetLineWidth('0.5mm');
        $this->pdf->cell(192, 3,'', 'BT', 0, 'L');
    }
}

