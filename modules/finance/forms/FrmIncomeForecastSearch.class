$MIOLO  = MIOLO::getInstance();
$module = MIOLO::getCurrentModule();
 
$MIOLO->uses('forms/FrmIncomeForecastReportSearch.class', $module);
/**
 * Form to search the finInvoice table
 */
class FrmIncomeForecastSearch extends FrmIncomeForecastReportSearch
{
    private $home;
    private $module;
     /**
     * Class constructor
     */
    function __construct($data)
    {
        $MIOLO        = MIOLO::getInstance();
        $module       = MIOLO::getCurrentModule();
        $this->module = $module;

        $this->home          = $data->home;
        $this->enableToolbar = true;
        parent::__construct(_M('Search', $module));
    }
}

