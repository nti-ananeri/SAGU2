/**
 * Research database classes
 */

$MIOLO->uses('types.class', 'basic');

class RshQuestion extends BasLog
{
    public $questionId;
    public $formId;
    public $questionCategoryId;
    public $description;
    public $isMulti = DB_FALSE;
    public $textType = DB_FALSE;
    public $allowAnswerComment = DB_FALSE;
    public $isByCurricularComponent = DB_FALSE;
    public $questionOrder;
}

class RshAnswer extends BasLog
{
    public $answerId;
    public $personId;
    public $optionId;
    public $questionId;
    public $optionText;
    public $optionComment;
}

class RshForm extends BasLog
{
    public $formId;
    public $description;
    public $shortDescription;
    public $isIdentified = DB_TRUE;
}

class RshOption extends BasLog
{
    public $optionId;
    public $questionId;
    public $description;
}

class RshQuestionCategory extends BasLog
{
    public $questionCategoryId;
    public $description;
}

class RshFormSetting extends BasLog
{
    public $formSettingId;
    public $formId;
    public $beginDate;
    public $endDate;
    public $beginHour;
    public $endHour;
    public $isStudent;
    public $isProfessor;
    public $isEmployee;
    public $isInstitutionalEvaluation;
    public $classId;
    public $courseId;
    public $courseVersion;
    public $turnId;
    public $unitId;
}

class RshAnswerInstitutionalEvaluation extends BasLog
{
    public $answerId;
    public $personId;
    public $enrollId;
    public $optionId;
    public $questionId;
    public $optionText;
    public $optionComment;
}

